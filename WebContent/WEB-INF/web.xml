<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Login</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Controller</servlet-name>
    <servlet-class>com.controller.Controller</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AdminCreatesUser</servlet-name>
    <servlet-class>com.controller.AdminCreatesUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AdminActions</servlet-name>
    <servlet-class>com.admin.AdminActions</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AdminMenuItemUpdate</servlet-name>
    <servlet-class>com.admin.AdminMenuItemUpdate</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AdminDeleteUser</servlet-name>
    <servlet-class>com.admin.AdminDeleteUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AdminVendorUpdate</servlet-name>
    <servlet-class>com.admin.AdminVendorUpdate</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DbConnection</servlet-name>
    <servlet-class>com.admin.DbConnection</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Controller</servlet-name>
    <url-pattern>/controller</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>cafeterialdetail</servlet-name>
    <servlet-class>com.controller.cafeterialdetail</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cafeterialdetail</servlet-name>
    <url-pattern>/cafeterialdetail</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FoodItemDetail</servlet-name>
    <servlet-class>com.controller.FoodItemDetail</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FoodItemDetail</servlet-name>
    <url-pattern>/FoodItemDetail</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AdminCreatesUser</servlet-name>
    <url-pattern>/AdminCreatesUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AdminActions</servlet-name>
    <url-pattern>/AdminActions</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AdminMenuItemUpdate</servlet-name>
    <url-pattern>/AdminMenuItemUpdate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AdminDeleteUser</servlet-name>
    <url-pattern>/AdminDeleteUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AdminVendorUpdate</servlet-name>
    <url-pattern>/AdminVendorUpdate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DbConnection</servlet-name>
    <url-pattern>/DbConnection</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>10</session-timeout>
  </session-config>
</web-app>